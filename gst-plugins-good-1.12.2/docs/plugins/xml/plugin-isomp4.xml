<!DOCTYPE refentry PUBLIC "-//OASIS//DTD DocBook XML V4.1.2//EN" "http://www.oasis-open.org/docbook/xml/4.1.2/docbookx.dtd">
<refentry id="gst-plugins-good-plugins-plugin-isomp4">
  <refmeta>
    <refentrytitle>isomp4</refentrytitle>
    <manvolnum>3</manvolnum>
    <refmiscinfo>FIXME Library</refmiscinfo>
  </refmeta>
  <refnamediv>
    <refname>isomp4</refname>
    <refpurpose><anchor id="plugin-isomp4"/>ISO base media file format support (mp4, 3gpp, qt, mj2)</refpurpose>
  </refnamediv>
  <refsect1>
    <title>Plugin Information</title>
    <variablelist>
      <varlistentry>
        <term>filename</term>
        <listitem>
          <simpara>libgstisomp4.so</simpara>
        </listitem>
      </varlistentry>
      <varlistentry>
        <term>version</term>
        <listitem>
          <simpara>1.12.2</simpara>
        </listitem>
      </varlistentry>
      <varlistentry>
        <term>run-time license</term>
        <listitem>
          <simpara>LGPL</simpara>
        </listitem>
      </varlistentry>
      <varlistentry>
        <term>package</term>
        <listitem>
          <simpara>GStreamer Good Plug-ins source release</simpara>
        </listitem>
      </varlistentry>
      <varlistentry>
        <term>origin</term>
        <listitem>
          <simpara>Unknown package origin</simpara>
        </listitem>
      </varlistentry>
    </variablelist>
  </refsect1>
  <refsect1>
    <title>Elements</title>
    <variablelist>
    <varlistentry><term><link linkend="gst-plugins-good-plugins-3gppmux">3gppmux</link></term><listitem><simpara>Multiplex audio and video into a 3GPP file</simpara></listitem></varlistentry>
    <varlistentry><term><link linkend="gst-plugins-good-plugins-ismlmux">ismlmux</link></term><listitem><simpara>Multiplex audio and video into a ISML file</simpara></listitem></varlistentry>
    <varlistentry><term><link linkend="gst-plugins-good-plugins-mj2mux">mj2mux</link></term><listitem><simpara>Multiplex audio and video into a MJ2 file</simpara></listitem></varlistentry>
    <varlistentry><term><link linkend="gst-plugins-good-plugins-mp4mux">mp4mux</link></term><listitem><simpara>Multiplex audio and video into a MP4 file</simpara></listitem></varlistentry>
    <varlistentry><term><link linkend="gst-plugins-good-plugins-qtdemux">qtdemux</link></term><listitem><simpara>Demultiplex a QuickTime file into audio and video streams</simpara></listitem></varlistentry>
    <varlistentry><term><link linkend="gst-plugins-good-plugins-qtmoovrecover">qtmoovrecover</link></term><listitem><simpara>Recovers unfinished qtmux files</simpara></listitem></varlistentry>
    <varlistentry><term><link linkend="gst-plugins-good-plugins-qtmux">qtmux</link></term><listitem><simpara>Multiplex audio and video into a QuickTime file</simpara></listitem></varlistentry>
    <varlistentry><term><link linkend="gst-plugins-good-plugins-rtpxqtdepay">rtpxqtdepay</link></term><listitem><simpara>Extracts Quicktime audio/video from RTP packets</simpara></listitem></varlistentry>
  </variablelist>
  </refsect1>
</refentry>
