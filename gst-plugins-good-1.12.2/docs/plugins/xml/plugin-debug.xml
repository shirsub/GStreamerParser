    <varlistentry><term><link linkend="gst-plugins-good-plugins-progressreport">progressreport</link></term><listitem><simpara>Periodically query and report on processing progress</simpara></listitem></varlistentry>
    <varlistentry><term><link linkend="gst-plugins-good-plugins-pushfilesrc">pushfilesrc</link></term><listitem><simpara>Implements pushfile:// URI-handler for push-based file access</simpara></listitem></varlistentry>
    <varlistentry><term><link linkend="gst-plugins-good-plugins-rndbuffersize">rndbuffersize</link></term><listitem><simpara>pull random sized buffers</simpara></listitem></varlistentry>
    <varlistentry><term><link linkend="gst-plugins-good-plugins-taginject">taginject</link></term><listitem><simpara>inject metadata tags</simpara></listitem></varlistentry>
    <varlistentry><term><link linkend="gst-plugins-good-plugins-testsink">testsink</link></term><listitem><simpara>perform a number of tests</simpara></listitem></varlistentry>
